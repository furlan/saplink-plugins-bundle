            msg = &apos;illegal_language&apos;.
      WHEN OTHERS.
        RAISE EXCEPTION TYPE zcx_saplink
          EXPORTING
            textid = zcx_saplink=&gt;error_message
            msg = &apos;Error&apos;.
    ENDCASE.

  ENDIF.

endmethod.</source>
