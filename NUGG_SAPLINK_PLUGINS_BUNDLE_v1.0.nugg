*      lowtzow@gmx.de

  DATA xdokhl TYPE dokhl.
  DATA xdoc_object(62) TYPE c.
  DATA xdocu_id TYPE dokhl-id.
  DATA xdocu_obj TYPE dokhl-object.

  xdoc_object = objname.
*  xdocu_id  = xdoc_object(2).
*  xdocu_obj = xdoc_object+2(60).
  xdocu_id = getdocid( ).
  xdocu_obj = xdoc_object.

  CLEAR xdokhl.
